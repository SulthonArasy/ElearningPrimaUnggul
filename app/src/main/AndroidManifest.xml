<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.sulthon.elearningprimaunggul">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/pu"
        android:roundIcon="@drawable/pu"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.pelajaran.PelajaranActivity" />
        <activity android:name=".ui.listmateri.ActivityListMateri" />
        <activity android:name=".ui.uploadmateri.UploadMateriActivity" />
        <activity android:name=".ui.about.AboutActivity" />
        <activity android:name=".ui.inputsoal.InputSoalActivity" />
        <activity android:name=".ui.nilai.ActivityNilai" />
        <activity android:name=".ui.soal.SoalActivity" />
        <activity android:name=".ui.materi.MateriActivity" />
        <activity android:name=".ui.listsoal.ListSoalActivity"></activity>
    </application>

</manifest>